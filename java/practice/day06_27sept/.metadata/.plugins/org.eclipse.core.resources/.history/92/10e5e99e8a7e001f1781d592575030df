package com.cdac;

public class DayEnumTester {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		DayEnum tomorrow = DayEnum.MONDAY;
		System.out.println(tomorrow);
		
		String str = "FRIDAY";
		//DayEnum d2 = DayEnum(str);  // error: we can't instantiate enums
		
		DayEnum d3 = DayEnum.valueOf(str);  // here valueOf(str)== FRIDAY that's why no error
		System.out.println(d3);
		
	}

}
